
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">

<head>
    <link rel="stylesheet" href="~/AdminLTE-3.0.4/plugins/fontawesome-free/css/all.min.css">
    <link rel="stylesheet" href="http://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <link rel="stylesheet" href="~/AdminLTE-3.0.4/dist/css/adminlte.min.css">
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <title>Web Application</title>
    <link href="~/quixlab-main/theme/css/style.css" rel="stylesheet">

</head>

<body>
    <div id="preloader">
        <div class="loader">

        </div>
    </div>
    <div id="main-wrapper">
        <div class="nav-header">
            <div class="brand-logo" height="2000" width="1000">
                <a href="~/Person/GetDeletedPersonList/">
                    <b class="logo-abbr"><img src="images/logo.png" alt=""> </b>
                    <span class="logo-compact"><img src="./images/logo-compact.png" alt=""></span>
                    <span class="brand-title">
                        <h2 style="color:white">Web App</h2>
                    </span>
                </a>
            </div>
        </div>
        <div class="header">
            <div class="header-content clearfix">

                <div class="nav-control">
                    <div class="hamburger">
                        <span class="toggle-icon"><i class="icon-menu"></i></span>
                    </div>
                </div>
                <div class="header-left">
                    <div class="input-group icons">
                    </div>
                </div>
            </div>
        </div>
        <div class="nk-sidebar">
            <div class="nk-nav-scroll">
                <ul class="metismenu" id="menu">
                    <li class="nav-label">İşlemler</li>
                    <li>
                        <a href="/Person/GetPersonList/" aria-expanded="false">
                            <i class="icon-envelope menu-icon"></i> <span class="nav-text">Üye Listesi</span>
                        </a>

                    </li>
                    <li>
                        <a href="/Person/AddPerson/" aria-expanded="false">
                            <i class="icon-screen-tablet menu-icon" href="/Person/AddPerson/"></i><span class="nav-text">Üye Ekle</span>
                        </a>

                    </li>
                    <li>
                        <a href="/Person/GetDeletedPersonList/" aria-expanded="false">
                            <i class="icon-graph menu-icon"></i> <span class="nav-text">Silinen Üyeler</span>
                        </a>
                    </li>
                </ul>
            </div>
        </div>
        <div class="content-body">
            <div class="container-fluid">
                @RenderBody()
            </div>
        </div>
        <div class="footer">
            <div class="copyright">
                <p> &copy; Geliştirici  <a href="/Person/GetPersonList/">Web Applicaton</a> 2023</p>
            </div>
        </div>
    </div>
    <script src="~/quixlab-main/theme/plugins/common/common.min.js"></script>
    <script src="~/quixlab-main/theme/js/custom.min.js"></script>
    <script src="~/quixlab-main/theme/js/settings.js"></script>
    <script src="~/quixlab-main/theme/js/gleek.js"></script>
    <script src="~/quixlab-main/theme/js/styleSwitcher.js"></script>
    <script src="~/AdminLTE-3.0.4/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/AdminLTE-3.0.4/dist/js/adminlte.js"></script>
    <script src="~/AdminLTE-3.0.4/plugins/chart.js/Chart.min.js"></script>
    <script src="~/AdminLTE-3.0.4/dist/js/demo.js"></script>
    <script src="~/AdminLTE-3.0.4/dist/js/pages/dashboard3.js"></script>
</body>

</html>
